{
    "version": "1.0.0-*",
    "description": "FocalLengthAnalyzer Console Application",
    "authors": [ "Marcin" ],
    
    "buildOptions": {
        "emitEntryPoint": true,
        "warningsAsErrors": true,
        "define": [ "NOPERSISTENTREADER", "NOPARALLEL", "NOMEMORYREADER" ]
    },

    "commands": {
        "FocalLengthAnalyzer": "FocalLengthAnalyzer"
    },

    "packOptions": {
        "tags": [ "exif", "focal length" ],
        "projectUrl": "https://github.com/mkorz/FocalLengthAnalyzer",
        "licenseUrl": "http://www.gnu.org/licenses/"
    },

    "frameworks": {
        "netcoreapp1.0": { "imports": [ "dnxcore50","portable-net45+win8" ] }
    },

    "dependencies": {
        "Microsoft.NETCore.App": {
            "type": "platform",
            "version": "1.0.0-rc2-3002702"
        }
    }
}
